def jumpingOnClouds(c):
    count = 0
    i = 0
    while i <= (len(c)-1):
        if i == len(c)-1:
            return count
        else:
            if c[i+1] == 0:
                if (i+1) == len(c)-1 or c[i+2] == 1:
                    i = i + 1
                else:
                    i = i + 2
            elif c[i+1] == 1:
                i = i + 2
           
        count = count + 1
            
    return count
